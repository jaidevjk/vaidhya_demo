
    <section >
    <form method="post" encType="multipart/form-data" action='/myFirstApp.AESoftwareSolution/Unni/SibiN.12.02.1996.JameS/AikarA/Add-superImage'>
     <button type="submit" class="btn btn-success mt-4">Add Image</button>
     <br>
        <div class="card" style="width: 15rem; height: 15rem" >
     <b>
                    <p></p>
                    <img src="" alt="np found" style="width:200px; height:200px" id="imgView" class="pt-3 pb-3">
                    <br>
                    <p></p>
                    <br>
                    <input type="File" name="Image" class="form-control" onchange="viewImage(event)">
     </div>
     
    </form>
    <hr size="30">
        <div class="conatiner mt-5">
            <div class="row">
                {{#each superImage}}
                <div class=col-md-3 p-5>
                    <div class="card" style="width: 15rem; height: 15rem" >
                        <img style="height: 15rem"; src="/uploads/superImage/{{this.imgName}}" alt="No image Found" class="card-img-top">
                        <div>
                            <a href="/myFirstApp.AESoftwareSolution/Unni/SibiN.12.02.1996.JameS/AikarA/delete-superImage/{{this._id}}/{{this.imgName}}" class="btn btn-danger" onclick="return  confirm('are you want to delete {{this.imgName}}')">Delete</a>
                        </div> 
                    </div>
                    <br>
                </div>
                {{/each }}
            </div>
        </div>
    </section>
    <script>
 function viewImage(event){
        document.getElementById('imgView').src=URL.createObjectURL(event.target.files[0]) //to access new image
    }
    </script>

